{
    "name": "@24consulting/interview-frontend",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --port 3000",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint .7src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "start": "json-server-auth --watch json-server/db.json --port 3001",
        "start-dev": "concurrently \"npm run dev\" \"npm run start\""
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.45.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "concurrently": "^7.5.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^9.8.0",
        "json-server": "^0.17.1",
        "json-server-auth": "^2.1.0",
        "postcss": "^8.4.19",
        "prettier": "^2.7.1",
        "sass": "^1.56.1",
        "stylelint": "^14.16.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-standard-scss": "^6.1.0",
        "typescript": "^4.8.4",
        "vite": "^3.2.5",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-stylelint": "^3.0.8"
    },
    "dependencies": {
        "@types/node": "^18.11.10",
        "@vitejs/plugin-vue": "^3.2.0",
        "pina": "^0.20.2204228",
        "sass-loader": "^13.2.0",
        "vue": "^3.2.45",
        "vue-style-loader": "^4.1.3",
        "vue-tsc": "^1.0.11"
    }
}
